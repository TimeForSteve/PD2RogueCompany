    <bodies>
        <body enabled="true" template="animated" name="lid_collision">
        	<object name="lid_joint"/>
            <object name="c_box_01" collision_type="box" />
        </body>
		<body enabled="true" template="static" name="case_collision">
            <object name="rp_cargo_bomb_case"/>
            <object name="c_box_02" collision_type="box" />
        </body>
    </bodies>

    <animations>
	    <animation_group name="anim" enabled="false">
            <object name="case_joint" />
			<object name="lid_joint" />
	    </animation_group>
	</animations>

    

    <graphics>
        <graphic_group name="case" enabled="true" culling_object="co_culling">
            <object name="g_case_lid" enabled="true" />
            <object name="g_case_bottom" enabled="true" />
            <object name="g_case_bottom_empty" enabled="false" />
            <object name="g_details" enabled="true" />
        </graphic_group>
    </graphics>
</dynamic_object>
