<dynamic_object>
	<diesel materials="units/pd2_dlc_dark/equipment/gen_drone_camera/gen_drone_camera" orientation_object="rp_gen_drone_camera" />
	<sequence_manager file="units/pd2_dlc_dark/equipment/gen_drone_camera/gen_drone_camera" />
	<animation_def name="anims/units/level/dark/drone/def" />
	<bodies>
		<body name="body_static" enabled="true" template="animated" collides_with_mover="false" pushed_by_mover="false" >
			<object name="CameraPitch" />
			<object name="c_box_01" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_hitbox" enabled="false" template="animated" collides_with_mover="false" pushed_by_mover="false" >
			<object name="CameraPitch" />
			<object name="c_box_01" collision_type="box" padding="-2.5" />
		</body>
	</bodies>
	<decal_surfaces default_material="metal_chassis">
		<decal_mesh name="g_body" enabled="true" material="metal_chassis" />
	</decal_surfaces>

	<effects>
		<effect_spawner name="es_sparks" enabled="false" object="e_sparks" effect="effects/payday2/particles/smoke_trail/drone_smoke" />

		<effect_spawner name="es_lights_red_01" enabled="false" object="e_effect_01" effect="effects/payday2/particles/flares/drone_flare_red" />
		<effect_spawner name="es_lights_red_02" enabled="false" object="e_effect_02" effect="effects/payday2/particles/flares/drone_flare_red" />
		<effect_spawner name="es_lights_red_03" enabled="false" object="e_effect_03" effect="effects/payday2/particles/flares/drone_flare_red" />
		<effect_spawner name="es_lights_red_04" enabled="false" object="e_effect_04" effect="effects/payday2/particles/flares/drone_flare_red" />

		<effect_spawner name="es_lights_white_01" enabled="false" object="e_effect_01" effect="effects/payday2/particles/flares/drone_flare_white" />
		<effect_spawner name="es_lights_white_02" enabled="false" object="e_effect_02" effect="effects/payday2/particles/flares/drone_flare_white" />
		<effect_spawner name="es_lights_white_03" enabled="false" object="e_effect_03" effect="effects/payday2/particles/flares/drone_flare_white" />
		<effect_spawner name="es_lights_white_04" enabled="false" object="e_effect_04" effect="effects/payday2/particles/flares/drone_flare_white" />

		<effect_spawner name="es_lights_blue_01" enabled="false" object="e_effect_01" effect="effects/payday2/particles/flares/drone_flare_blue" />
		<effect_spawner name="es_lights_blue_02" enabled="false" object="e_effect_02" effect="effects/payday2/particles/flares/drone_flare_blue" />
		<effect_spawner name="es_lights_blue_03" enabled="false" object="e_effect_03" effect="effects/payday2/particles/flares/drone_flare_blue" />
		<effect_spawner name="es_lights_blue_04" enabled="false" object="e_effect_04" effect="effects/payday2/particles/flares/drone_flare_blue" />

	</effects>

	<graphics>
		<object name="g_body" enabled="true" />
		<object name="g_blade_1" enabled="false" />
		<object name="g_blade_2" enabled="false" />
		<object name="g_blade_3" enabled="false" />
		<object name="g_blade_4" enabled="false" />
		<object name="g_blade_solid_1" enabled="true" />
		<object name="g_blade_solid_2" enabled="true" />
		<object name="g_blade_solid_3" enabled="true" />
		<object name="g_blade_solid_4" enabled="true" />

		<object name="g_body_dw" enabled="false"/>
		<object name="g_led_01" enabled="false"/>
		<object name="g_led_02" enabled="false"/>
		<object name="g_led_03" enabled="false"/>

	</graphics>
	<lights>
		<light name="li_light_red" enabled="false" editable="true" type="spot|specular" multiplier="identity" far_range="350" falloff="4" />
		<light name="li_light_blue" enabled="false" editable="true" type="spot|specular" multiplier="identity" far_range="350" falloff="4" />
		<light name="li_light_white" enabled="false" editable="true" type="spot|specular" multiplier="identity" far_range="350" falloff="4" />
	</lights>
</dynamic_object>
