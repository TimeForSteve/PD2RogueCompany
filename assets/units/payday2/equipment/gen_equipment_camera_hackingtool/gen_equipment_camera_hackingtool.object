<dynamic_object>
    <diesel materials="units/payday2/equipment/gen_equipment_camera_hackingtool/gen_equipment_camera_hackingtool" orientation_object="rp_gen_equipment_camera_hackingtool" />
	<sequence_manager file="units/payday2/equipment/gen_equipment_camera_hackingtool/gen_equipment_camera_hackingtool" />
    <bodies>
        <body enabled="true" template="editor" name="static_body" >
            <object collision_type="box" name="c_box" padding="-2.5" />
        </body>
    </bodies>
    <graphics>

		<graphic_group name="drillicongroup" 	enabled="false" culling_object="g_icon">
			<lod_object name="drill_icon">
					<object name="g_icon" 		enabled="true"  max_distance="450"/>
					<object name="g_icon_lod1" 	enabled="true"/>
			</lod_object>
		</graphic_group>
        <graphic_group name="grp_tool" enabled="true" culling_object="g_tool">
            <lod_object name="lod_tool">
                <object name="g_tool"       enabled="true"  max_distance="500" />
                <object name="g_tool_lod"   enabled="true" />
            </lod_object>
        </graphic_group>
        <object enabled="false" name="g_tool_glow" />
    </graphics>
</dynamic_object>
