<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/payday2/props/stn_wall_fence_gate_picklock/stn_wall_fence_gate_picklock" orientation_object="rp_stn_wall_fence_gate_picklock" />
	<sequence_manager file="units/payday2/props/stn_wall_fence_gate_picklock/stn_wall_fence_gate_picklock" />
	<animations>
		<animation_group name="anim">
			<object name="anim_right_fence" />
			<object name="anim_hinge1" />
			<object name="anim_hinge2" />
			<object name="anim_left_fence" />
		</animation_group>
	</animations>
	<bodies>
		<body name="body_left_fence" enabled="true" template="bag" keyframed="true">
			<object name="anim_left_fence" />
			<object name="c_box_left" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_right_fence" enabled="true" template="bag" keyframed="true">
			<object name="anim_right_fence" />
			<object name="c_box_right" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_padlock" enabled="true" template="static" collides_with_mover="false">
			<object name="c_padlock" collision_type="box" />
		</body>
	</bodies>
	<decal_surfaces default_material="fence">
	</decal_surfaces>
	<graphics>
		<graphic_group name="icon" enabled="true" culling_object="g_icon_lock">
			<lod_object name="lock_icon">
				<object name="g_icon_lock" 			enabled="true"  max_distance="450"  max_draw_lod="0" />
				<object name="g_icon_lock_lod1" 		enabled="true"  lod="1" />
			</lod_object>
		</graphic_group>
		<graphic_group name="fence" enabled="true" culling_object="co_cull">
			<object name="g_fence" enabled="true"  />
		</graphic_group>
		<object name="g_lock" 	enabled="true"  />
	</graphics>
</dynamic_object>