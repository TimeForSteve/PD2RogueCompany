<dynamic_object>
    <diesel materials="units/payday2/vehicles/str_vehicle_truck_frontloader/str_vehicle_truck_frontloader" orientation_object="rp_str_vehicle_truck_frontloader" />
    <bodies>
        <body enabled="true" template="static" name="static_body" >
            <object collision_type="mesh_mopp" name="c_c" />
        </body>
        <body name="body_blocker_graph" enabled="true" template="graph_blocker">
			<object name="rp_str_vehicle_truck_frontloader" />
			<object name="c_graph_blocker" collision_type="box" padding="25" />
		</body>
    </bodies>
    <decal_surfaces>
        <decal_mesh enabled="true" name="dm_glass" material="glass_breakable" />
        <decal_mesh enabled="true" name="dm_body" material="sheet_metal" />
        <decal_mesh enabled="true" name="dm_tires" material="rubber" />
    </decal_surfaces>
    <graphics>
        <lod_object name="lod_truck">
            <object name="g_truck">
                <material materials="mtr_frontloader" max_distance="2000"/>
                <material materials="mtr_frontloader_lod_1" max_distance="4000"/>
                <material materials="mtr_frontloader_lod_2" />
            </object>
        </lod_object>
        <object enabled="true" name="g_truck" />
        <object enabled="true" name="g_decals" />
        <object enabled="true" name="g_glass" />
        <object enabled="true" shadow_caster="true" name="s_s" />
    </graphics>
</dynamic_object>
